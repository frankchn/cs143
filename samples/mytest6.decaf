void main() {
  int[] a;
  int i;
  int sz;
  
  sz = 0;
  
  while(sz < 2) {
    Print("Enter Number: ");
    sz = ReadInteger();
  }
  
  a = NewArray(sz, int);
  a[0] = 0;
  a[1] = 1;
  
  for(i = 2; i < a.length(); i++) {
    a[i] = a[i-1] + a[i-2];
  }
  
  for(i = 0; i < a.length(); i++) {
    Print(a[i], " ");
  }
}
